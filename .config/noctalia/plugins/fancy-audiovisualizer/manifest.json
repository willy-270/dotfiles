{
  "id": "fancy-audiovisualizer",
  "name": "Fancy Audiovisualizer",
  "version": "1.0.7",
  "minNoctaliaVersion": "3.7.2",
  "author": "Noctalia Team",
  "official": true,
  "license": "MIT",
  "repository": "https://github.com/noctalia-dev/noctalia-plugins",
  "description": "Lemmy's fancy circular audio visualizer.",
  "tags": ["Desktop", "Audio"],
  "entryPoints": {
    "desktopWidget": "DesktopWidget.qml",
    "settings": "Settings.qml"
  },
  "dependencies": {
    "plugins": []
  },
  "metadata": {
    "defaultSettings": {
      "sensitivity": 1.5,
      "rotationSpeed": 0.5,
      "barWidth": 0.6,
      "ringOpacity": 0.8,
      "bloomIntensity": 0.5,
      "visualizationMode": 3,
      "waveThickness": 1.0,
      "innerDiameter": 0.7,
      "fadeWhenIdle": false,
      "useCustomColors": false,
      "customPrimaryColor": "#6750A4",
      "customSecondaryColor": "#625B71"
    }
  }
}
